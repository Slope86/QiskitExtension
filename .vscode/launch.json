{
    "version": "0.2.0",
    "configurations": [{
            "name": "Python: Current File with profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o", "debug.prof", "${file}"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}